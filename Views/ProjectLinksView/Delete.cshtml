@model GymLinkPro.Models.ProjectLink

@{
    ViewData["Title"] = "Delete Project Link";
}

<h1>Delete Project Link</h1>

<h3>Are you sure you want to delete this link?</h3>
<div>
    <h4>ProjectLink</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">ProjectId</dt>
        <dd class="col-sm-10">@Model.ProjectId</dd>
        <dt class="col-sm-2">Url</dt>
        <dd class="col-sm-10">@Model.Url</dd>
        <dt class="col-sm-2">Description</dt>
        <dd class="col-sm-10">@Model.Description</dd>
    </dl>
</div>

<form asp-action="Delete" method="post">
    <input type="hidden" asp-for="ProjectLinkId" />
    <button type="submit" class="btn btn-danger">Delete</button>
    <a asp-action="Index" class="btn btn-secondary">Back to List</a>
</form>