@model GymLinkPro.Models.ProjectMember

@{
    ViewData["Title"] = "Delete Project Member";
}

<h1>Delete Project Member</h1>

<h3>Are you sure you want to delete this member?</h3>
<div>
    <h4>ProjectMember</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">MemberId</dt>
        <dd class="col-sm-10">@Model.MemberId</dd>
        <dt class="col-sm-2">ProjectId</dt>
        <dd class="col-sm-10">@Model.ProjectId</dd>
        <dt class="col-sm-2">Role</dt>
        <dd class="col-sm-10">@Model.Role</dd>
    </dl>
</div>

<form asp-action="Delete" method="post">
    <input type="hidden" asp-for="ProjectMemberId" />
    <button type="submit" class="btn btn-danger">Delete</button>
    <a asp-action="Index" class="btn btn-secondary">Back to List</a>
</form>